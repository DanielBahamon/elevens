<% local = Club.find_by(id: duel.club_id) %>
<article class="_prev" id="_prev-<%= duel.id %>">
	<section class="_cont _1">
    <div class="_1">
      <div class="_i">
				<p class="_mutted"><%= duel.sport %></p>
      </div>
      <div class="_ii">
				<p class="_mutted">Expires <%= calculate_event_time(duel) %></p>
      </div>
    </div>
		<div class="_2">
      <div class="_i">
      	<div class="_in">
					<%= link_to panel_club_duel_path(local, duel), class: "_typematch", target: "_blank" do %>
						<h5 class="_title">This event is incomplete</h5>
					<% end %>
      	</div>
      </div>
      <div class="_ii">
				<%= link_to panel_club_duel_path(local, duel), class: "button--naira _1", target: "_blank" do %>
      		<i class="button__icon fad fa-external-link"></i>
      		<span>Complete</span>
				<% end %>
      </div>
		</div>
		<div class="_3">
			<div class="_i">
	      <div class="_data">
					<%= link_to panel_club_duel_path(local, duel), class: "_typematch", target: "_blank" do %>
						<% if duel.duel_type == "Friendly" %>
		          <figure>
								<i class="fad fa-handshake-alt"></i>
		          </figure>
		          <p class="_annun">Friendly</p>
						<% elsif duel.duel_type == "Bet" %>
		          <figure>
								<i class="fad fa-sack-dollar"></i>
		          </figure>
		          <p class="_annun">Bet</p>
						<% elsif duel.duel_type == "Tournament" %>
		          <figure>
								<i class="fad fa-trophy-alt"></i>
		          </figure>
		          <p class="_annun">Tournament</p>
						<% elsif duel.duel_type == "League" %>
		          <figure>
								<i class="fad fa-award"></i>
		          </figure>
		          <p class="_annun">League</p>
						<% elsif duel.duel_type == "Mundial" %>
		          <figure>
								<i class="fal fa-globe-americas"></i>
		          </figure>
		          <p class="_annun">World Cup</p>
						<% end %>
					<% end %>
	      </div>
			</div>
			<div class="_ii"></div>
		</div>
	</section>
</article>